{
  "editor.fontFamily": "SF Mono",
  "editor.fontSize": 15,
  "editor.lineHeight": 26,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "comments": "#888888",
  },
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*.html": "html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true
  },
  "[html]": {
    "editor.formatOnSave": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.tabSize": 4
  },
  "terminal.integrated.fontSize": 15,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "sideBar.foreground": "#fafafa",
    "sideBar.background": "#282828",
    "sideBar.border": "#262626",
    "sideBarSectionHeader.foreground": "#fafafa",
    "editor.background": "#202020",
    "editorGutter.background": "#202020",
    "editorLineNumber.foreground": "#666666",
    "editorLineNumber.activeForeground": "#fafafa",
    "editorSuggestWidget.background": "#1c1c1c",
    "editorSuggestWidget.selectedBackground": "#282828",
    "editor.lineHighlightBackground": "#282828",
    "editorWidget.background": "#181818",
    "editorCursor.foreground": "#f59e0b",
    "terminal.background": "#202020",
    "activityBar.background": "#202020",
    "activityBar.foreground": "#fafafa",
    "statusBar.background": "#0f0f0f",
    "statusBar.foreground": "#fafafa",
    "tab.activeBackground": "#333333",
    "tab.activeForeground": "#dddddd",
    "tab.inactiveBackground": "#181818",
    "tab.inactiveForeground": "#dddddd",
    "tab.hoverBackground": "#505050",
    "editorGroupHeader.tabsBackground": "#181818",
    "sideBarSectionHeader.background": "#181818",
    "scrollbarSlider.background": "#444444",
    "scrollbar.shadow": "#282828",
    "scrollbarSlider.hoverBackground": "#555555",
    "scrollbarSlider.activeBackground": "#555555",
    "breadcrumb.background": "#101010",
    "breadcrumb.focusForeground": "#fafafa",
    "breadcrumbPicker.background": "#101010",
    "breadcrumb.foreground": "#999999",
    "button.background": "#fafafa",
    "button.foreground": "#444444",
    "statusBarItem.hoverBackground": "#505050",
    "list.hoverBackground": "#505050",
    "list.activeSelectionBackground": "#505050",
    "list.inactiveSelectionForeground": "#fafafa",
    "gitDecoration.ignoredResourceForeground": "#999999",
    "gitDecoration.modifiedResourceForeground": "#93c5fd"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Ace Palenight"
}
