{
  "editor.fontFamily": "Iosevka SS07 Extended",
  "editor.fontSize": 15,
  "editor.lineHeight": 24,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "comments": "#888888"
  },
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*.html": "html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true
  },
  "[html]": {
    "editor.formatOnSave": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.tabSize": 4
  },
  "terminal.integrated.fontSize": 15,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "focusBorder": "#333333",
    "foreground": "#ffffff",
    "sideBar.foreground": "#ffffff",
    "sideBar.background": "#282828",
    "sideBar.border": "#262626",
    "sideBarSectionHeader.foreground": "#ffffff",
    "editor.background": "#202020",
    "editorGutter.background": "#202020",
    "editorLineNumber.foreground": "#555555",
    "editorLineNumber.activeForeground": "#cacaca",
    "editorSuggestWidget.background": "#1c1c1c",
    "editorSuggestWidget.selectedBackground": "#282828",
    "editor.lineHighlightBackground": "#282828",
    "editorWidget.background": "#181818",
    "editorCursor.foreground": "#f59e0b",
    "terminal.background": "#202020",
    "activityBar.background": "#202020",
    "activityBar.foreground": "#ffffff",
    "statusBar.background": "#065F46",
    "statusBar.foreground": "#ffffff",
    "tab.activeBackground": "#333333",
    "tab.activeForeground": "#dddddd",
    "tab.inactiveBackground": "#202020",
    "tab.inactiveForeground": "#dddddd",
    "tab.hoverBackground": "#505050",
    "editorGroupHeader.tabsBackground": "#202020",
    "sideBarSectionHeader.background": "#202020",
    "scrollbarSlider.background": "#444444",
    "scrollbar.shadow": "#282828",
    "scrollbarSlider.hoverBackground": "#555555",
    "scrollbarSlider.activeBackground": "#555555",
    "breadcrumb.background": "#101010",
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumbPicker.background": "#101010",
    "breadcrumb.foreground": "#999999",
    "button.background": "#333333",
    "button.foreground": "#ffffff",
    "statusBarItem.hoverBackground": "#333333",
    "statusBar.noFolderBackground": "#3B82F6",
    "statusBar.noFolderForeground": "#ffffff",
    "list.hoverBackground": "#505050",
    "list.hoverForeground": "#ffffff",
    "list.activeSelectionBackground": "#505050",
    "list.inactiveSelectionForeground": "#ffffff",
    "gitDecoration.ignoredResourceForeground": "#999999",
    "gitDecoration.modifiedResourceForeground": "#93c5fd",
    "input.background": "#333333",
    "input.foreground": "#ffffff",
    "settings.textInputBackground": "#333333",
    "settings.textInputForeground": "#ffffff",
    "settings.numberInputBackground": "#333333",
    "settings.numberInputForeground": "#ffffff",
    "settings.dropdownBackground": "#333333",
    "settings.dropdownForeground": "#ffffff",
    "welcomePage.background": "#202020",
    "welcomePage.buttonBackground": "#333333",
    "icon.foreground": "#ffffff",
    "panel.background": "#181818"
  },
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "bearded-icons",
  "workbench.colorTheme": "Atom Material Theme",
  "scm.inputFontSize": 14,
  "workbench.tree.indent": 20
}
